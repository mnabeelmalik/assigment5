   "source": [
    "def palind(data):\n",
    "    left_p = 0\n",
    "    right_p = len(data) - 1\n",
    "    \n",
    "    while right_p >= left_p:\n",
    "        if not data[left_p] == data[right_p]:\n",
    "            return \"NO\"\n",
    "        left_p += 1\n",
    "        right_p -= 1\n",
    "    return \"Yes it is palindrome\"\n",
    "print(palind('MAHAM')) "
   ]
